[Unit]
Description=Humidity Logger Backend
After=network.target

[Service]
ExecStart=/home/pi/humidityLogger/backend/index.js
Restart=always
User=nobody
Group=nobody
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/home/pi/humidityLogger/backend/

[Install]
WantedBy=multi-user.target
